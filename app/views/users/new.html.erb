<h1>회원가입</h1>
<%= form_for @user do |f| %>
    Nickname: <%= f.text_field :nickname %><br>
    Email: <%= f.email_field :email %><br>
    Password: <%= f.password_field :password %><br>
    Password Confirmation: <%= f.password_field :password_confirmation %><br>
    Name: <%= f.text_field :Name %><br>
    Gender: <%= f.select :Gender, raw("<option>male</option><option>female</option>") %><br>
    Birth Date: <%= f.date_field :Birth_Date %><br>
    <%= f.submit '회원가입' %>
<% end %>